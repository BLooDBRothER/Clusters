{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/new",
        "destination": "/index.html"
      },
      {
        "source": "/myposts",
        "destination": "/index.html"
      },
      {
        "source": "/bookmarks",
        "destination": "/index.html"
      }
    ]
  }
}
